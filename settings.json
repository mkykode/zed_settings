{
  "ssh_connections": [
    {
      "host": "goldenstate",
      "projects": [
        {
          "paths": ["/bitnami"]
        }
      ]
    },
    {
      "host": "findyourparadise",
      "projects": [
        {
          "paths": ["/bitnami/wordpress"]
        }
      ]
    },
    {
      "host": "shltr",
      "projects": [
        {
          "paths": ["/bitnami/wordpress"]
        }
      ]
    }
  ],
  "features": {
    "edit_prediction_provider": "zed"
  },
  "telemetry": {
    "diagnostics": false,
    "metrics": false
  },
  "theme": "Catppuccin Mocha",
  "buffer_font_size": 13.0,
  "buffer_font_features": {
    "calt": true,
    "liga": true
  },
  "autosave": "on_focus_change",
  "buffer_font_family": "Monaspace Neon Var",
  "ui_font_family": "Monaspace Neon Var",
  "vim_mode": true,
  "command_aliases": {
    "W": "w",
    "Wq": "wq",
    "Q": "q"
  },
  "ui_font_features": {
    "calt": true,
    "liga": true
  },
  "relative_line_numbers": true,
  "project_panel": {
    "dock": "right"
  },
  "auto_update": true,
  "terminal": {
    "font_family": "MesloLGS NF",
    "font_size": 12
  },
  "agent": {
    "always_allow_tool_actions": true,
    "default_profile": "write",
    "default_model": {
      "provider": "zed.dev",
      "model": "claude-opus-4-1-thinking"
    }
  },
  "format_on_save": "on",
  "soft_wrap": "preferred_line_length",
  "show_wrap_guides": true,
  "wrap_guides": [80, 100],
  "preferred_line_length": 100,
  "tabs": {
    "file_icons": true
  },
  "lsp": {
    "tailwindcss-language-server": {
      "settings": {
        "includeLanguages": {
          "liquid": "html"
        }
      }
    }
  },
  "languages": {
    "JavaScript": {
      "formatter": {
        "language_server": {
          "name": "biome"
        }
      },
      "language_servers": ["typescript-language-server", "biome"],
      "code_actions_on_format": {
        "source.organizeImports.biome": true,
        "source.fixAll.biome": true
      }
    },
    "TypeScript": {
      "formatter": {
        "language_server": {
          "name": "biome"
        }
      },
      "language_servers": ["typescript-language-server", "biome"]
    },
    "TSX": {
      "formatter": {
        "language_server": {
          "name": "biome"
        }
      },
      "language_servers": ["typescript-language-server", "biome"]
    },
    "CSS": {
      "formatter": {
        "language_server": {
          "name": "biome"
        }
      },
      "language_servers": ["biome", "tailwindcss-language-server"]
    },
    "SCSS": {
      "formatter": {
        "language_server": {
          "name": "biome"
        }
      },
      "language_servers": ["biome"]
    },
    "HTML": {
      "formatter": "prettier",
      "language_servers": ["!biome", "..."]
    },
    "JSON": {
      "formatter": {
        "language_server": {
          "name": "biome"
        }
      },
      "language_servers": ["biome"]
    },
    "YAML": {
      "formatter": "prettier"
    },
    "Markdown": {
      "formatter": "prettier"
    },
    "Ruby": {
      "language_servers": ["ruby-lsp", "rubocop", "..."],
      "formatter": "language_server"
    },
    "PHP": {
      "format_on_save": "off",
      "formatter": {
        "external": {
          "command": "/Users/jullweber/.local/bin/phpcbf-wrapper",
          "arguments": ["-"]
        }
      },
      "language_servers": ["phpactor"]
    },
    "Rust": {
      "format_on_save": "on",
      "formatter": "language_server"
    },
    "Liquid": {
      "formatter": "prettier",
      "prettier": {
        "allowed": true,
        "plugins": ["@shopify/prettier-plugin-liquid", "prettier-plugin-tailwindcss"]
      },
      "language_servers": ["tailwindcss-language-server", "..."]
    },
    "Nix": {
      "language_servers": ["/run/current-system/sw/bin/nixd"],
      "formatter": {
        "external": {
          "command": "/run/current-system/sw/bin/alejandra",
          "arguments": ["--quiet", "--"]
        }
      }
    },
    "env": {
      "language_servers": ["/run/current-system/sw/bin/dotenv-linter"]
    },
    "Python": {
      "language_servers": ["ruff"],
      "format_on_save": "on",
      "formatter": "language_server"
    }
  },
  "active_pane_modifiers": {
    "inactive_opacity": 0.95
  },
  "diagnostics": {
    "include_warnings": true,
    "inline": {
      "enabled": true
    }
  },
  "auto_indent_on_paste": false,
  "preview_tabs": {
    "enabled": true
  },
  "context_servers": {
    "mcp-server-brave-search": {
      "source": "extension",
      "settings": {}
    },
    "eslint": {
      "source": "custom",
      "command": "/Users/jullweber/.volta/bin/npx",
      "args": ["eslint", "--mcp"]
    },
    "awslabs.aws-documentation-mcp-server": {
      "source": "custom",
      "command": "/run/current-system/sw/bin/uvx",
      "args": ["awslabs.aws-documentation-mcp-server@latest"],
      "env": {
        "FASTMCP_LOG_LEVEL": "ERROR"
      }
    },
    "prismic-mcp-server": {
      "source": "custom",
      "command": "npx",
      "args": ["-y", "@prismicio/mcp-server"]
    }
  }
}
